# 主构建目标
main-build: Game.elf secondary-outputs

# 工具调用
Game.elf Game.map: $(OBJS) $(USER_OBJS) C:\Users\Cyphe\Documents\SUSTech\ Assignment\Term\ 5\Embedded\ System\Assignment\Assignment\ 4\Code\STM32F103RCTX_FLASH.ld makefile objects.list $(OPTIONAL_TOOL_DEPS)
	arm-none-eabi-gcc -o "Game.elf" @"objects.list" $(USER_OBJS) $(LIBS) -mcpu=cortex-m3 -T"C:\Users\Cyphe\Documents\SUSTech Assignment\Term 5\Embedded System\Assignment\Assignment 4\Code\STM32F103RCTX_FLASH.ld" --specs=nosys.specs -Wl,-Map="Game.map" -Wl,--gc-sections -static --specs=nano.specs -mfloat-abi=soft -mthumb -Wl,--start-group -lc -lm -Wl,--end-group
	@echo '已结束构建目标： $@'
	@echo ' '

default.size.stdout: $(EXECUTABLES) makefile objects.list $(OPTIONAL_TOOL_DEPS)
	arm-none-eabi-size  $(EXECUTABLES)
	@echo '已结束构建： $@'
	@echo ' '

Game.list: $(EXECUTABLES) makefile objects.list $(OPTIONAL_TOOL_DEPS)
	arm-none-eabi-objdump -h -S $(EXECUTABLES) > "Game.list"
	@echo '已结束构建： $@'
	@echo ' '

Game.hex: $(EXECUTABLES) makefile objects.list $(OPTIONAL_TOOL_DEPS)
	arm-none-eabi-objcopy  -O ihex $(EXECUTABLES) "Game.hex"
	@echo '已结束构建： $@'
	@echo ' '

# 其他目标
clean:
	-$(RM) Game.elf Game.hex Game.list Game.map default.size.stdout
	-@echo ' '

secondary-outputs: $(SIZE_OUTPUT) $(OBJDUMP_LIST) $(OBJCOPY_HEX)

